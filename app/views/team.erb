<h1>Team Name and Heroes</h1>
<p>Team Name: <%=@team_name %></p>
<p>Team Motto: <%=@team_motto %></p>

<%@superheroes.each.with_index do |info, index| %>
<p><h2>Hero's Name: <%=@superhero_name[index] %></h2></p>
<p><h2>Hero's Power: <%=@superhero_power[index] %></h2></p>
<p><h2>Hero's Bio: <%=@superhero_bio[index] %></h2></p>
<%end%>
